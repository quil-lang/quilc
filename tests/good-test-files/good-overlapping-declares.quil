DECLARE c2 INTEGER SHARING b
DECLARE c3 INTEGER SHARING d

DECLARE d0 REAL SHARING d
DECLARE d1 REAL SHARING a
DECLARE d2 REAL SHARING b
DECLARE d3 REAL SHARING c
